<%- include('./partials/header') %>
    <%- include('./partials/flashMessage') %>
        <div class="w-full md:w-1/2 px-[5vw] sm:px-5 mt-20 sm:mt-0 mx-auto flex items-center justify-center h-auto">
            <div class="w-full">
                <h4 class="text-xl capitalize mb-5">Owner Login</h4>
                <form autocomplete="off" action="/owners/login" method="post">
                    <input
                        class="block bg-zinc-100 w-full px-3 py-2 border-[1px] rounded-md mb-3 border-zinc-200 outline-blue-500"
                        type="email" placeholder="Email" name="email">
                    <input
                        class="block bg-zinc-100 w-full px-3 py-2 border-[1px] rounded-md mb-3 border-zinc-200 outline-blue-500"
                        type="password" placeholder="Password" name="password">
                    <input class="px-5 block rounded-full py-2 mt-2 bg-blue-500 text-white" type="submit" value="Login">
                </form>
            </div>
        </div>
        <script src="/javascripts/flashMessage.js"></script>
        <%- include('./partials/footer') %>